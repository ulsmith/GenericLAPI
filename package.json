{
	"name": "generic-lapi",
	"version": "1.0.0",
	"description": "An AWS Lambda stack API for a Generic application",
	"author": "Paul Smith",
	"license": "copyright 2018 ulsmith all rights reserved",
	"private": true,
	"scripts": {
		"start": "sam local start-api --host 0.0.0.0 --template template.yaml",
		"package": "sam package --template-file template.yaml --s3-bucket ulsmith-cloudformation-build --output-template-file packaged.template.yaml",
		"deploy": "sam deploy --template-file packaged.template.yaml --stack-name ulsmith-generic-api --capabilities CAPABILITY_IAM --region eu-west-2"
	},
	"dependencies": {
		"aws-lambda-multipart-parser": "^0.1.3",
		"crypto-js": "^3.1.9-1",
		"jsonwebtoken": "^8.5.1",
		"knex": "^0.20.8",
		"mysql": "^2.17.1",
		"pg": "^7.12.1",
		"request": "^2.88.0",
		"request-promise-native": "^1.0.5"
	},
	"devDependencies": {},
	"engines": {
		"node": ">=10.0"
	}
}
